<% @races.each do |ndx, race| %>	<div class="race">		<span class="race_name"><%=race[:name]%></span>		<table>			<tr>				<th></th>				<th>Starts on (UTC)</th>			</tr>						<% race[:stages].each do |stage|%>				<tr>					<td><span class="stage_name"><%=stage.name%></span></td>					<td><span class="start_dt" id="<%=stage.id%>"><input type="text" placeholder="yyyy-mm-dd hh:mm:ss"></span></td>				</tr>			<% end %>		</table>	</div><% end %><hr/><button onclick="submit_season_stages();">Save</button><script>function submit_season_stages() {	season_stage_info = [];	$('.start_dt').each(function(ndx, elt) {		info = {};		info['stage_id'] = $(elt).attr('id');		info['start_dt'] = $(elt).find('input').val();		season_stage_info.push(info);	});		$.post('/admin/save_season_stages', {season_stage_info:season_stage_info}, function(response) {		console.log(response);	});}</script><style>div.race .race_name {	font-weight: bold;	text-decoration: underline;}ul.stages li div {	float: left;}</style>